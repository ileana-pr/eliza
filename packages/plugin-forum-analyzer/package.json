  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/jest": "^29.5.11",
    "@types/natural": "^5.1.5",
    "@types/cheerio": "^0.22.35",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
